        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Coordinator_example"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid>
        <Button Content="OpenNewScreenButton" Command="{Binding OpenNewScreenCommand}" HorizontalAlignment="Left" Margin="363,144,0,0" VerticalAlignment="Top"/>
        <Button Content="OpenMessageButton" Command="{Binding ShowMessageBoxCommand}" HorizontalAlignment="Left" Margin="403,217,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
